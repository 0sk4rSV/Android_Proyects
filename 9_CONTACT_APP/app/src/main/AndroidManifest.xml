<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.act8_bd_sqlite_osv">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Contacts"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.act8_bd_sqlite_osv.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.act8_bd_sqlite_osv.CrearContacto"></activity>
        <activity android:name="com.example.act8_bd_sqlite_osv.InfoContacto"></activity>
        <activity android:name="com.example.act8_bd_sqlite_osv.EditContacto"></activity>
    </application>

</manifest>